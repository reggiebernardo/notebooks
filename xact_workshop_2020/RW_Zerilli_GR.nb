Cell[TextData[{
 "Before defining the perturbed spacetime, here are a two more definitions \
that will aid us in our calculations. First, we define the function \
\[OpenCurlyDoubleQuote]",
 StyleBox["linearize",
  FontColor->RGBColor[1, 0.5, 0.5]],
 "\[CloseCurlyDoubleQuote] which series expands an expression in terms of the \
",
 StyleBox["perturbation parameter \[Epsilon]",
  FontColor->RGBColor[1, 0.5, 0.5]],
 " and then organizes the result."
}], "Text",
 CellChangeTimes->{{3.7720907888365293`*^9, 3.7720909802152767`*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"DefConstantSymbol", "[", "\[Epsilon]", "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"linearize", "[", "expr_", "]"}], ":=", 
  RowBox[{"Collect", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Series", "[", 
      RowBox[{"expr", ",", 
       RowBox[{"{", 
        RowBox[{"\[Epsilon]", ",", "0", ",", "1"}], "}"}]}], "]"}], "//", 
     "Normal"}], ",", "\[Epsilon]", ",", "Together"}], "]"}]}]}], "Input",
 CellChangeTimes->{3.772090992757265*^9}],

Cell[TextData[{
 "Second (and last), we define the ",
 StyleBox["spherical harmonics",
  FontColor->RGBColor[1, 0.5, 0.5]],
 " ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["Y", "lm"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " (see Arfken 7th ed. page 757)."
}], "Text",
 CellChangeTimes->{{3.7720910025150747`*^9, 3.77209104795531*^9}, {
  3.7720911005707655`*^9, 3.772091103904677*^9}, {3.772091426040887*^9, 
  3.772091445592239*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"DefConstantSymbol", "/@", 
   RowBox[{"{", 
    RowBox[{"l", ",", "m", ",", "\[Omega]"}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"DefScalarFunction", "[", 
    RowBox[{"PL", ",", 
     RowBox[{
     "PrintAs", "\[Rule]", "\"\<\!\(\*SubscriptBox[\(P\), \(lm\)]\)\>\""}]}], 
    "]"}], ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"associated", " ", "Legendre", " ", "polynomials"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"PL", "''"}], "[", "\[Theta]_", "]"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"l", 
          RowBox[{"(", 
           RowBox[{"l", "+", "1"}], ")"}]}], "-", 
         RowBox[{
          SuperscriptBox["m", "2"], " ", 
          SuperscriptBox[
           RowBox[{"Csc", "[", "\[Theta]", "]"}], "2"]}]}], ")"}]}], " ", 
      RowBox[{"PL", "[", "\[Theta]", "]"}]}], "-", 
     RowBox[{
      RowBox[{"Cot", "[", "\[Theta]", "]"}], 
      RowBox[{
       RowBox[{"PL", "'"}], "[", "\[Theta]", "]"}]}]}]}], ";"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"spherical", " ", "harmonics"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"YLM", ":=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"\[Sqrt]", 
        RowBox[{"(", 
         RowBox[{
          FractionBox[
           RowBox[{
            RowBox[{"2", "l"}], "+", "1"}], 
           RowBox[{"4", "\[Pi]"}]], 
          FractionBox[
           RowBox[{
            RowBox[{"(", 
             RowBox[{"l", "-", "m"}], ")"}], "!"}], 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"l", "+", "m"}], ")"}], "!"}]]}], ")"}]}], 
       SuperscriptBox["E", 
        RowBox[{
         RowBox[{"-", "I"}], " ", "m", " ", 
         RowBox[{"\[Phi]", "[", "]"}]}]], 
       RowBox[{"PL", "[", 
        RowBox[{"\[Theta]", "[", "]"}], "]"}]}], ")"}], 
     SuperscriptBox["E", 
      RowBox[{
       RowBox[{"-", "I"}], " ", "\[Omega]", " ", 
       RowBox[{"t", "[", "]"}]}]]}]}], ";"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"for", " ", "coloring", " ", "the", " ", "output"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ColorOut", "[", "expr_", "]"}], ":=", 
  RowBox[{"Style", "[", 
   RowBox[{
    RowBox[{"expr", "//", "ScreenDollarIndices"}], ",", 
    RowBox[{"FontColor", "\[Rule]", 
     RowBox[{"RGBColor", "[", 
      RowBox[{
       RowBox[{"1", "/", "2"}], ",", "1", ",", 
       RowBox[{"1", "/", "2"}]}], "]"}]}]}], "]"}]}]}], "Input",
