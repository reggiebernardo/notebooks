Cell[TextData[{
 "1. ",
 StyleBox["Linearize",
  FontSlant->"Italic"],
 " the following functions:\n\t(a) ",
 Cell[BoxData[
  FormBox[
   RowBox[{"sin", "(", 
    RowBox[{"\[Epsilon]", " ", "x"}], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontColor->RGBColor[1, 0.5, 0.5]],
 "\n\t(b) ",
 Cell[BoxData[
  FormBox[
   RowBox[{"cos", "(", 
    RowBox[{"\[Epsilon]", " ", "x"}], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontColor->RGBColor[1, 0.5, 0.5]],
 "\n\t(c) ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "/", 
    RowBox[{"(", 
     RowBox[{"1", "-", 
      RowBox[{"\[Epsilon]", " ", "x"}]}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontColor->RGBColor[1, 0.5, 0.5]],
 "\n\t(d) ",
 Cell[BoxData[
  FormBox[
   RowBox[{"exp", "(", 
    RowBox[{
     RowBox[{"-", "\[Epsilon]"}], " ", 
     SuperscriptBox["x", "2"]}], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontColor->RGBColor[1, 0.5, 0.5]],
 "\n\t\n2. Use ",
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{
     RowBox[{
      SubscriptBox["P", "lm"], "(", "\[Theta]", ")"}], "=", 
     RowBox[{"LegendreP", "(", 
      RowBox[{"l", ",", "m", ",", 
       RowBox[{"Cos", "(", "\[Theta]", ")"}]}], ")"}]}],
    FontColor->RGBColor[1, 0.5, 0.5]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " and prove the following identities for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"l", ",", "m"}], ")"}], ",", 
     RowBox[{"(", 
      RowBox[{"0", ",", "0"}], ")"}], ",", 
     RowBox[{"(", 
      RowBox[{"1", ",", "0"}], ")"}], ",", 
     RowBox[{"(", 
      RowBox[{"1", ",", 
       RowBox[{"-", "1"}]}], ")"}], ",", 
     RowBox[{"(", 
      RowBox[{"1", ",", "1"}], ")"}], ",", 
     RowBox[{"(", 
      RowBox[{"2", ",", "0"}], ")"}], ",", 
     RowBox[{"(", 
      RowBox[{"2", ",", 
       RowBox[{"-", "1"}]}], ")"}], ",", 
     RowBox[{"(", 
      RowBox[{"2", ",", 
       RowBox[{"-", "2"}]}], ")"}], ",", 
     RowBox[{"(", 
      RowBox[{"2", ",", "1"}], ")"}], ",", 
     RowBox[{"(", 
      RowBox[{"2", ",", "2"}], ")"}]}], "}"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ":\n\n\t(a) ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      SubscriptBox["\[Integral]", "0"], "\[Pi]"], 
     RowBox[{
      SuperscriptBox[
       RowBox[{
        SubscriptBox["P", "lm"], "(", 
        RowBox[{"cos", "(", "\[Theta]", ")"}], ")"}], "2"], 
      RowBox[{"\[DifferentialD]", "\[CapitalOmega]"}]}]}], "=", 
    FractionBox[
     RowBox[{"2", 
      RowBox[{"(", 
       RowBox[{"2", "\[Pi]"}], ")"}]}], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", "l"}], "+", "1"}], ")"}]]}], TraditionalForm]],
  FontColor->RGBColor[1, 0.5, 0.5]],
 "\n\t(b) ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      SubscriptBox["\[Integral]", "0"], "\[Pi]"], 
     RowBox[{
      RowBox[{
       SubscriptBox["P", "lm"], "'"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"cos", "(", "\[Theta]", ")"}], ")"}], "2"], 
      RowBox[{"\[DifferentialD]", "\[CapitalOmega]"}]}]}], "=", 
    FractionBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{"2", "\[Pi]"}], ")"}], "2", 
      RowBox[{"l", "(", 
       RowBox[{"l", "+", "1"}], ")"}]}], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", "l"}], "+", "1"}], ")"}]]}], TraditionalForm]],
  FontColor->RGBColor[1, 0.5, 0.5]],
 "\n\t\nwhere ",
 Cell[BoxData[
  FormBox[
   RowBox[{"d\[CapitalOmega]", "=", 
    RowBox[{"2", "\[Pi]", " ", 
     RowBox[{"sin", "(", "\[Theta]", ")"}], "d\[Theta]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Text",
 CellChangeTimes->{{3.805972697076394*^9, 3.8059728527651043`*^9}, {
