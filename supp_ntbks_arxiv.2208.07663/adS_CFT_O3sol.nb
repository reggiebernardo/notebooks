          RowBox[{"r", "[", "]"}], "]"}]}], ",", 
        FractionBox[
         SuperscriptBox["L", "2"], 
         RowBox[{
          SuperscriptBox[
           RowBox[{"r", "[", "]"}], "2"], 
          RowBox[{"F", "[", 
           RowBox[{"r", "[", "]"}], "]"}]}]], ",", 
