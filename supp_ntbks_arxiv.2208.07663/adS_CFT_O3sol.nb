  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"AdS", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"H", "\[Rule]", 
        RowBox[{"Function", "[", 
         RowBox[{"r", ",", 
          RowBox[{"1", "-", 
           FractionBox[
            SuperscriptBox["M", "3"], 
            SuperscriptBox["r", "3"]], "+", 
           FractionBox[
            SuperscriptBox["L", "2"], 
            SuperscriptBox["r", "2"]]}]}], "]"}]}], ",", 
       RowBox[{"F", "\[Rule]", 
        RowBox[{"Function", "[", 
         RowBox[{"r", ",", 
          RowBox[{"1", "-", 
           FractionBox[
            SuperscriptBox["M", "3"], 
            SuperscriptBox["r", "3"]], "+", 
           FractionBox[
            SuperscriptBox["L", "2"], 
            SuperscriptBox["r", "2"]]}]}], "]"}]}]}], "}"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
