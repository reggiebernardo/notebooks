  RowBox[{
   RowBox[{
    RowBox[{"DefChart", "[", 
     RowBox[{"FRW", ",", "Md", ",", 
      RowBox[{"{", 
       RowBox[{"0", ",", "1", ",", "2", ",", "3"}], "}"}], ",", 
      RowBox[{"{", 
