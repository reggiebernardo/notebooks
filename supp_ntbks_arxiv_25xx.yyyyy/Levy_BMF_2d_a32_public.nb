Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"R", "[", 
    RowBox[{"x_", ",", "y_"}], "]"}], "=", 
   SqrtBox[
    RowBox[{
     SuperscriptBox["x", "2"], "+", 
     SuperscriptBox["y", "2"]}]]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"\[Xi]f", "[", 
     RowBox[{"x_", ",", "y_"}], "]"}], "=", 
    FractionBox["1", 
     SqrtBox[
      RowBox[{"R", "[", 
       RowBox[{"x", ",", "y"}], "]"}]]]}], ";"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"FT", " ", "of", " ", "correlation"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Xi]t", "[", "k_", "]"}], "=", 
   RowBox[{"Simplify", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"FT2d", "[", 
       RowBox[{"\[Xi]f", "[", 
        RowBox[{"x", ",", "y"}], "]"}], "]"}], "/.", 
      RowBox[{
       RowBox[{
        SuperscriptBox["u", "2"], "+", 
        SuperscriptBox["v", "2"]}], "->", 
       SuperscriptBox["k", "2"]}]}], ",", 
     RowBox[{"Assumptions", "\[Rule]", 
      RowBox[{"k", ">=", "0"}]}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"\"\<\!\(\*OverscriptBox[\(\[Xi]\), \(~\)]\)(k)\>\"", "==", 
  RowBox[{"\[Xi]t", "[", "k", "]"}]}]}], "Input",
 CellChangeTimes->{{3.9192981635917273`*^9, 3.9192981774790277`*^9}, {
