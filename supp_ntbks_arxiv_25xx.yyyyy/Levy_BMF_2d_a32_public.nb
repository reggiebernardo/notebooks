Cell["\<\
On the other hand, the one-point skewness of the density can be derived from \
the cumulants by a series expansion;\
\>", "Text",
 CellChangeTimes->{{3.9249284933552437`*^9, 3.9249285056661825`*^9}, {
  3.9249289967032433`*^9, 3.924929056719366*^9}, {3.9499115828142533`*^9, 
  3.94991158420014*^9}},ExpressionUUID->"4870e92c-255e-484a-82ce-\
9d4ce1a625ca"],

Cell[BoxData[
 RowBox[{
  RowBox[{"GetS3", "[", "k_", "]"}], ":=", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{
     RowBox[{"cgf", "[", 
      RowBox[{"\[Lambda]_", ",", "\[Xi]_"}], "]"}], "=", 
     RowBox[{"GetCGF", "[", 
      RowBox[{"k", ",", "\[Lambda]", ",", "\[Xi]"}], "]"}]}], ";", 
    RowBox[{
     RowBox[{"cgfExpand", "[", 
      RowBox[{"\[Lambda]_", ",", "\[Xi]_"}], "]"}], "=", 
     RowBox[{
      RowBox[{"Series", "[", 
       RowBox[{
        RowBox[{"cgf", "[", 
         RowBox[{"\[Lambda]", ",", "\[Xi]"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Lambda]", ",", "0", ",", "3"}], "}"}]}], "]"}], "//", 
      "Normal"}]}], ";", 
    RowBox[{"c2", "=", 
     RowBox[{
      RowBox[{"2", "!"}], 
      RowBox[{"Coefficient", "[", 
       RowBox[{
        RowBox[{"cgfExpand", "[", 
         RowBox[{"\[Lambda]", ",", "\[Xi]"}], "]"}], ",", "\[Lambda]", ",", 
        "2"}], "]"}]}]}], ";", 
    RowBox[{"c3", "=", 
     RowBox[{
      RowBox[{"3", "!"}], 
      RowBox[{"Coefficient", "[", 
       RowBox[{
        RowBox[{"cgfExpand", "[", 
         RowBox[{"\[Lambda]", ",", "\[Xi]"}], "]"}], ",", "\[Lambda]", ",", 
        "3"}], "]"}]}]}], ";", 
    RowBox[{"S3", "=", 
     RowBox[{
      FractionBox["c3", 
       SuperscriptBox["c2", "2"]], "//", "N"}]}], ";", "S3"}], 
   ")"}]}]], "Input",
 CellChangeTimes->{{3.9248418238562016`*^9, 3.9248418751881056`*^9}, {
