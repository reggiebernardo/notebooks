947342137dc6"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Xi0", "=", "4.5"}], ";"}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Rho]min", "=", 
   SuperscriptBox["10", 
    RowBox[{"-", "1"}]]}], ";", 
  RowBox[{"\[Rho]max", "=", "20"}], ";", 
  RowBox[{"\[CapitalDelta]\[Rho]", "=", 
   FractionBox[
    RowBox[{"\[Rho]max", "-", "\[Rho]min"}], "200"]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"nPDF\[Rho]At\[Xi]0", "=", 
   RowBox[{"nPDF\[Rho]", "[", 
    RowBox[{"Xi0", ",", "\[Rho]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"tPDF\[Rho]At\[Xi]0", "=", 
   RowBox[{"tPDF\[Rho]", "[", 
    RowBox[{"\[Rho]", ",", "Xi0"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"BMFtoMF", "=", 
   RowBox[{
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"\[Rho]", "//", "N"}], ",", 
        FractionBox["nPDF\[Rho]At\[Xi]0", "tPDF\[Rho]At\[Xi]0"]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
       "\[Rho]", ",", "\[Rho]min", ",", "\[Rho]max", ",", 
        "\[CapitalDelta]\[Rho]"}], "}"}]}], "]"}], "//", "Quiet"}]}], 
  ";"}]}], "Input",
