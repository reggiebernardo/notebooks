Cell[TextData[{
 "Finally we can get the cumulants up to our desired order beyond mean field. \
We setup a function that does exactly this by solving the linear system of \
equation (equations C31 and C33 in ",
 ButtonBox["https://arxiv.org/abs/2402.15915)",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://arxiv.org/abs/2402.15915"], None},
  ButtonNote->"https://arxiv.org/abs/2402.15915"],
 ":"
}], "Text",
 CellChangeTimes->{{3.9199055359869666`*^9, 3.919905578965227*^9}, {
  3.9199056986435213`*^9, 3.919905757531102*^9}, {3.9499114889992056`*^9, 
  3.9499115362595673`*^9}},ExpressionUUID->"92adb6b4-e50f-467e-91cd-\
b152e7744f53"],

Cell[BoxData[
 RowBox[{
  RowBox[{"GetCGF", "[", 
   RowBox[{"L_", ",", "\[Lambda]\[Lambda]_", ",", "\[Xi]\[Xi]_"}], "]"}], ":=", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"eqC31sol0", "=", 
     RowBox[{"Solve", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"eqC31", "[", "L", "]"}], "/.", "\[CapitalXi]RulesUnion"}], 
       ",", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"\[Tau]", "[", "l", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"l", ",", "0", ",", "L"}], "}"}]}], "]"}]}], "]"}]}], ";", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"eqC33", "[", 
        RowBox[{"[", "2", "]"}], "]"}], "/.", 
       RowBox[{"eqC31sol0", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], "/.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"\[Lambda]", "[", "0", "]"}], "->", "\[Lambda]\[Lambda]"}], 
        ",", 
        RowBox[{"\[Xi]0", "->", "\[Xi]\[Xi]"}]}], "}"}]}], "//", 
