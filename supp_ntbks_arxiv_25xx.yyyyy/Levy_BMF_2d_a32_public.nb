 RowBox[{
  RowBox[{"\[CapitalXi]Rules0", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"\[CapitalXi]", "[", 
       RowBox[{"p", ",", "q"}], "]"}], "->", 
      RowBox[{"\[Xi]0", " ", 
       RowBox[{"\[CapitalXi]TabL0", "[", 
        RowBox[{"[", 
         RowBox[{
          RowBox[{"p", "+", "1"}], ",", 
          RowBox[{"q", "+", "1"}]}], "]"}], "]"}]}]}], ",", 
     RowBox[{"{", 
      RowBox[{"p", ",", "0", ",", "L"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"q", ",", "0", ",", "L"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
