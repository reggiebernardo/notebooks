Cell[CellGroupData[{

Cell["3 PDF of the density", "Subsection",
 CellChangeTimes->{{3.9199062736594205`*^9, 3.9199062987232113`*^9}, {
  3.924841709754908*^9, 
  3.9248417098803215`*^9}},ExpressionUUID->"c9c7fd27-70c8-4d75-868d-\
1f52183afe38"],

Cell["At mean field, the PDF of the density is given by", "Text",
 CellChangeTimes->{{3.9199066893497567`*^9, 
  3.919906708864524*^9}},ExpressionUUID->"a0964b97-266d-452f-914a-\
d7178fc3066f"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"tPDF\[Rho]", "[", 
    RowBox[{"\[Rho]_", ",", "\[Xi]0_"}], "]"}], "=", 
   FractionBox[
    RowBox[{"4", " ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"-", 
       FractionBox[
        RowBox[{"2", " ", 
         RowBox[{"(", 
          RowBox[{"1", "+", "\[Rho]"}], ")"}]}], "\[Xi]0"]}]], " ", 
     RowBox[{"Hypergeometric0F1Regularized", "[", 
      RowBox[{"2", ",", 
       FractionBox[
        RowBox[{"4", " ", "\[Rho]"}], 
        SuperscriptBox["\[Xi]0", "2"]]}], "]"}]}], 
    SuperscriptBox["\[Xi]0", "2"]]}], ";"}]], "Input",
 CellChangeTimes->{{3.919906721399201*^9, 3.919906724888308*^9}},
 CellLabel->"In[30]:=",ExpressionUUID->"8b5148fb-c56c-443b-88c3-90fa09f9e20c"],

Cell[TextData[{
 "Now that the cumulants are at hand, we can easily compute theoretical PDF \
of \[Rho] beyond the mean field approximation. We take up to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"l", "=", "8"}], TraditionalForm]],ExpressionUUID->
  "7bc03b6c-5d43-4283-9cc7-38fe9e581b10"],
 " in the following, noting this can be extended to any order."
}], "Text",
 CellChangeTimes->{{3.9199065423805017`*^9, 3.919906657107594*^9}, {
  3.9199109248028*^9, 
  3.9199109271040754`*^9}},ExpressionUUID->"03f2b720-0a5e-4321-941c-\
8fc2c7e0433f"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"\[Phi]BMF", "[", 
     RowBox[{"\[Lambda]_", ",", "\[Xi]_"}], "]"}], "=", 
    RowBox[{"GetCGF", "[", 
     RowBox[{"8", ",", "\[Lambda]", ",", "\[Xi]"}], "]"}]}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
    "change", " ", "first", " ", "argument", " ", "to", " ", "desired", " ", 
     "BMF", " ", "order"}], ";", " ", 
    RowBox[{"L", "=", 
     RowBox[{"0", " ", "is", " ", "mean", " ", "field"}]}]}], " ", 
   "*)"}]}]], "Input",
 CellChangeTimes->{{3.919906779020179*^9, 3.919906792887557*^9}, {
   3.9199070737298703`*^9, 3.9199071292727404`*^9}, {3.919908012374082*^9, 
   3.919908016610969*^9}, {3.9199092564969854`*^9, 3.91990926208919*^9}, {
   3.919910066597127*^9, 3.919910066999442*^9}, {3.919910700652467*^9, 
   3.919910701453745*^9}, {3.923127763524392*^9, 3.9231277662808475`*^9}, 
   3.924850222443939*^9, {3.924852321493577*^9, 3.9248523215554886`*^9}, {
   3.924852377275878*^9, 3.9248524476732616`*^9}, {3.9248537577946568`*^9, 
   3.9248537578571625`*^9}, {3.924929152754278*^9, 3.924929152927541*^9}},
 CellLabel->"In[31]:=",ExpressionUUID->"d20400a4-7296-4801-8ba0-f0c6e70b0638"],

Cell["\<\
The way to the PDF is do an inverse Laplace transform. I setup a function \
doing a Riemann sum (of the inverse Laplace transform integral after one \
integration by parts) in the following:\
\>", "Text",
 CellChangeTimes->{{3.91990716040586*^9, 3.91990720517023*^9}, {
  3.9249292811595373`*^9, 3.9249292868713417`*^9}, {3.949911798159992*^9, 
  3.949911814554325*^9}},ExpressionUUID->"56c27d2c-7e36-4cad-b26a-\
d3dfaf14b830"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"\[Phi]BMF\[Rho]", "[", 
    RowBox[{"\[Lambda]_", ",", "\[Xi]_"}], "]"}], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[PartialD]", "\[Lambda]"], 
     RowBox[{"\[Phi]BMF", "[", 
      RowBox[{"\[Lambda]", ",", "\[Xi]"}], "]"}]}], "//", "Simplify"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"lmax", "=", "5000"}], ";", 
  RowBox[{"dlstep", "=", "0.05"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"t\[CurlyPhi]1", "[", "\[Xi]b_", "]"}], "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"I", " ", "\[Lambda]\[Rho]"}], ",", 
       RowBox[{
        RowBox[{"\[Phi]BMF\[Rho]", "[", 
         RowBox[{
          RowBox[{"\[Lambda]\[Rho]", " ", "I"}], ",", "\[Xi]b"}], "]"}], 
        RowBox[{"Exp", "[", 
         RowBox[{"\[Phi]BMF", "[", 
          RowBox[{
           RowBox[{"I", " ", "\[Lambda]\[Rho]"}], ",", "\[Xi]b"}], "]"}], 
         "]"}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Lambda]\[Rho]", ",", 
       RowBox[{
        RowBox[{"-", "lmax"}], "-", 
        RowBox[{"dlstep", "/", "2"}]}], ",", 
       RowBox[{"lmax", "+", 
        RowBox[{"dlstep", "/", "2"}]}], ",", "dlstep"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"nPDF\[Rho]", "[", 
    RowBox[{"\[Xi]_", ",", "\[Rho]_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"t\[CurlyPhi]1", "[", "\[Xi]", "]"}], "/.", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"aa_", ",", "cc_"}], "}"}], "\[Rule]", 
        RowBox[{
         RowBox[{"1", "/", "\[Rho]"}], " ", 
         RowBox[{"Exp", "[", 
          RowBox[{
           RowBox[{"-", "aa"}], " ", "\[Rho]"}], "]"}], 
         RowBox[{"cc", "/", 
          RowBox[{"(", 
           RowBox[{"2", " ", "Pi"}], ")"}]}], "  ", "dlstep"}]}]}], ")"}], "//",
      "Total"}], "//", "Re"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.9199093294699445`*^9, 3.9199093355633197`*^9}, {
   3.9199095039614716`*^9, 3.9199095177535505`*^9}, 3.919909892336748*^9, 
   3.919909925333363*^9, {3.9248429649604387`*^9, 3.9248430084717913`*^9}, {
   3.9248434922357*^9, 3.9248435057084713`*^9}, {3.924844492950327*^9, 
   3.9248445065842037`*^9}, {3.9249293042196836`*^9, 3.924929315114817*^9}},
 CellLabel->"In[32]:=",ExpressionUUID->"a9e875e6-b707-4d7b-90ca-0767596fc653"],

