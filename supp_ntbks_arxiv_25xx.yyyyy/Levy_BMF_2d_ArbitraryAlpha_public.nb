b0e36748411c"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"Lmax", "=", "4"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Table", "[", 
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"q", ">=", "p"}], ",", 
      RowBox[{"Integrate", "[", 
       RowBox[{
        RowBox[{"2", "\[Pi]", " ", "r", " ", 
         RowBox[{"\[ScriptCapitalC]0", "[", "r", "]"}], " ", 
         RowBox[{"b", "[", 
          RowBox[{"q", ",", "r"}], "]"}], 
         RowBox[{"b", "[", 
          RowBox[{"p", ",", "r"}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"r", ",", "0", ",", "\[Infinity]"}], "}"}]}], "]"}], ",", 
      "xx"}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"p", ",", "0", ",", "Lmax"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"q", ",", "0", ",", "Lmax"}], "}"}]}], "]"}], "//", 
  "MatrixForm"}]}], "Input",
 CellChangeTimes->{{3.9199009740023737`*^9, 3.9199010608168173`*^9}, {
  3.940388482530875*^9, 3.9403884826116524`*^9}, {3.9403886936588354`*^9, 
