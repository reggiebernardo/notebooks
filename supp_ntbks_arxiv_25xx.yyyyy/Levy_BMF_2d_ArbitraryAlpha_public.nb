Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"R", "[", 
    RowBox[{"x_", ",", "y_"}], "]"}], "=", 
   SqrtBox[
    RowBox[{
     SuperscriptBox["x", "2"], "+", 
     SuperscriptBox["y", "2"]}]]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"\[Xi]f", "[", 
     RowBox[{"x_", ",", "y_"}], "]"}], "=", 
    FractionBox["1", 
     SuperscriptBox[
      RowBox[{"R", "[", 
       RowBox[{"x", ",", "y"}], "]"}], "\[Beta]"]]}], ";"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"FT", " ", "of", " ", "correlation"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Xi]t", "[", "k_", "]"}], "=", 
   RowBox[{"Simplify", "[", 
