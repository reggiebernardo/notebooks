Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{"setup", " ", "by", " ", "Cyril", " ", "Pitrou"}], "*)"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"PerturbAction", "[", 
     RowBox[{"expr_", ",", 
      RowBox[{
       RowBox[{
        RowBox[{"g_", "?", "MetricQ"}], "[", 
        RowBox[{
         RowBox[{"a_", "?", "UpIndexQ"}], ",", 
         RowBox[{"b_", "?", "UpIndexQ"}]}], "]"}], "|", 
       RowBox[{
        RowBox[{"g_", "?", "MetricQ"}], "[", 
        RowBox[{
         RowBox[{"a_", "?", "DownIndexQ"}], ",", 
         RowBox[{"b_", "?", "DownIndexQ"}]}], "]"}]}]}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"pertexpr", ",", "res", ",", "dgloc", ",", 
        RowBox[{"(*", 
         RowBox[{"dummyloc", ","}], "*)"}], "hp"}], "}"}], ",", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{
        "We", " ", "define", " ", "the", " ", "metric", " ", "perturbation"}],
         ",", " ", 
        RowBox[{"if", " ", "not", " ", "defined", " ", "already"}]}], " ", 
       "*)"}], "\[IndentingNewLine]", 
